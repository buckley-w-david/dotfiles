[user]
	email = david@davidbuckley.ca
	name = David Buckley
[alias]
	logtree = log --graph --oneline --decorate --all
	is = ! git branch | fzf | xargs git switch
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[pull]
	rebase = false
[init]
	defaultBranch = master
[core]
	editor = nvim
