## Git Aliases
alias gpo="git branch | awk '/^\*/{print $NF}' | xargs git push origin"

alias gs='git status '
alias ga='git add '
alias gb='git branch '
alias gc='git commit'
alias gd='git diff'
alias go='git checkout '
alias gp='git push '

alias got='git '
alias get='git '

git config --global alias.logtree "log --graph --oneline --decorate --all"
